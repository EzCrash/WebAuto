<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="DHgate Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Linux"/>
    <parameter name="os_version" value="Ubuntu"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="73.0"/>
    <parameter name="url" value="https://www.dhgate.com/"/>

    <test name = "DHgate Home">
        <classes>
            <!--<class name="title.AmazonHome"/>-->
            <!--<class name="search.SearchTest"/>-->
            <class name="pages.SearchBar"/>
            <class name="pages.Title"/>
        </classes>
    </test>

</suite>